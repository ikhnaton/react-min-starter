{
  "name": "react-min-starter",
  "version": "0.0.1",
  "description": "Bare bones app to start working with React",
  "main": "server/server.js",
  "scripts": {
    "prestart": "./node_modules/.bin/webpack --config webpack.prod.config.js",
    "start": "node .",
    "dev": "./node_modules/.bin/concurrently --names \"nodemon,webpack\" -c \"blue,green\" \"./node_modules/.bin/nodemon --ignore www -e js,jsx .\" \"./node_modules/.bin/webpack\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "node . | ./node_modules/.bin/webpack"
  },
  "dependencies": {
    "@cloudant/cloudant": "^2.1.0",
    "axios": "^0.18.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.20.3",
    "webpack": "^3.1.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "engines": {
    "node": "=9.6.1"
  }
}
